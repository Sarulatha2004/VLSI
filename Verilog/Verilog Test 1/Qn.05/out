#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x587167de1020 .scope module, "tb_clk_divider" "tb_clk_divider" 2 2;
 .timescale 0 0;
v0x587167df3d30_0 .net "clk_100hz", 0 0, v0x587167de13e0_0;  1 drivers
v0x587167df3e00_0 .var "clk_50mhz", 0 0;
v0x587167df3ed0_0 .var "rst", 0 0;
S_0x587167de11b0 .scope module, "uut" "clk_divider" 2 7, 3 1 0, S_0x587167de1020;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk_50mhz";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 1 "clk_100hz";
v0x587167de13e0_0 .var "clk_100hz", 0 0;
v0x587167df3a70_0 .net "clk_50mhz", 0 0, v0x587167df3e00_0;  1 drivers
v0x587167df3b30_0 .var "count", 17 0;
v0x587167df3bf0_0 .net "rst", 0 0, v0x587167df3ed0_0;  1 drivers
E_0x587167da76a0 .event posedge, v0x587167df3bf0_0, v0x587167df3a70_0;
    .scope S_0x587167de11b0;
T_0 ;
    %wait E_0x587167da76a0;
    %load/vec4 v0x587167df3bf0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 18;
    %assign/vec4 v0x587167df3b30_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x587167de13e0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x587167df3b30_0;
    %pad/u 20;
    %cmpi/e 249999, 0, 20;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 0, 0, 18;
    %assign/vec4 v0x587167df3b30_0, 0;
    %load/vec4 v0x587167de13e0_0;
    %inv;
    %assign/vec4 v0x587167de13e0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x587167df3b30_0;
    %addi 1, 0, 18;
    %assign/vec4 v0x587167df3b30_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x587167de1020;
T_1 ;
    %delay 10, 0;
    %load/vec4 v0x587167df3e00_0;
    %inv;
    %store/vec4 v0x587167df3e00_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x587167de1020;
T_2 ;
    %vpi_call 2 16 "$dumpfile", "clk_div.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x587167df3e00_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x587167df3ed0_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x587167df3ed0_0, 0, 1;
    %delay 30000000, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x587167de1020;
T_3 ;
    %vpi_call 2 29 "$monitor", "Time=%0t | rst=%b  | clk_100hz=%b", $time, v0x587167df3ed0_0, v0x587167df3d30_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
