TD_memory.large_ele ;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %load/vec4a v0x5c6397d96030, 4;
    %store/vec4 v0x5c6397d95f30_0, 0, 14;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5c6397d46800_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5c6397d46800_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x5c6397d95f30_0;
    %ix/getv/s 4, v0x5c6397d46800_0;
    %load/vec4a v0x5c6397d96030, 4;
    %cmp/u;
    %jmp/0xz  T_0.2, 5;
    %ix/getv/s 4, v0x5c6397d46800_0;
    %load/vec4a v0x5c6397d96030, 4;
    %store/vec4 v0x5c6397d95f30_0, 0, 14;
T_0.2 ;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x5c6397d46800_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x5c6397d46800_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
S_0x5c6397d95990 .scope task, "mem_elements" "mem_elements" 2 8, 2 8 0, S_0x5c6397d46490;
 .timescale 0 0;
v0x5c6397d95b90_0 .var/i "j", 31 0;
TD_memory.mem_elements ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5c6397d95b90_0, 0, 32;
T_1.4 ;
    %load/vec4 v0x5c6397d95b90_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_1.5, 5;
    %load/vec4 v0x5c6397d95b90_0;
    %muli 10, 0, 32;
    %addi 5, 0, 32;
    %pad/s 14;
    %ix/getv/s 4, v0x5c6397d95b90_0;
    %store/vec4a v0x5c6397d96030, 4, 0;
    %vpi_call 2 14 "$display", "mem[%d]=%d", v0x5c6397d95b90_0, &A<v0x5c6397d96030, v0x5c6397d95b90_0 > {0 0 0};
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x5c6397d95b90_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x5c6397d95b90_0, 0, 32;
    %jmp T_1.4;
T_1.5 ;
    %end;
S_0x5c6397d95c70 .scope task, "sum_memory" "sum_memory" 2 18, 2 18 0, S_0x5c6397d46490;
 .timescale 0 0;
v0x5c6397d95e50_0 .var/i "k", 31 0;
TD_memory.sum_memory ;
    %pushi/vec4 0, 0, 18;
