#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5bdddb060740 .scope module, "tb_not" "tb_not" 2 1;
 .timescale 0 0;
v0x5bdddb084e00_0 .var "a", 0 0;
v0x5bdddb084ea0_0 .net "o", 0 0, L_0x5bdddb084f70;  1 drivers
S_0x5bdddb03b4c0 .scope module, "uut" "not_gate" 2 7, 3 1 0, S_0x5bdddb060740;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /OUTPUT 1 "o";
L_0x5bdddb084f70 .functor NOT 1, v0x5bdddb084e00_0, C4<0>, C4<0>, C4<0>;
v0x5bdddb03b6a0_0 .net "a", 0 0, v0x5bdddb084e00_0;  1 drivers
v0x5bdddb084ce0_0 .net "o", 0 0, L_0x5bdddb084f70;  alias, 1 drivers
    .scope S_0x5bdddb060740;
T_0 ;
    %vpi_call 2 10 "$dumpfile", "not_wave.vcd" {0 0 0};
    %vpi_call 2 11 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5bdddb060740 {0 0 0};
    %vpi_call 2 13 "$display", "time | a b | o" {0 0 0};
    %vpi_call 2 14 "$monitor", "%2t | %b  | %b", $time, v0x5bdddb084e00_0, v0x5bdddb084ea0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5bdddb084e00_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5bdddb084e00_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 18 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_not_gate.v";
    "not_datal.v";
