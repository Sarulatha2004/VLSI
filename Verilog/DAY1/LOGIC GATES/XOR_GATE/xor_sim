#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x606db0218820 .scope module, "tb_xor" "tb_xor" 2 1;
 .timescale 0 0;
v0x606db023ec30_0 .var "a", 0 0;
v0x606db023ed00_0 .var "b", 0 0;
v0x606db023edd0_0 .net "o", 0 0, L_0x606db023eed0;  1 drivers
S_0x606db022e030 .scope module, "uut" "xor_gate" 2 5, 3 1 0, S_0x606db0218820;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "o";
L_0x606db023eed0 .functor XOR 1, v0x606db023ec30_0, v0x606db023ed00_0, C4<0>, C4<0>;
v0x606db022e260_0 .net "a", 0 0, v0x606db023ec30_0;  1 drivers
v0x606db023ea50_0 .net "b", 0 0, v0x606db023ed00_0;  1 drivers
v0x606db023eb10_0 .net "o", 0 0, L_0x606db023eed0;  alias, 1 drivers
    .scope S_0x606db0218820;
T_0 ;
    %vpi_call 2 8 "$dumpfile", "xor_wave.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x606db0218820 {0 0 0};
    %vpi_call 2 11 "$display", "time | a b | o" {0 0 0};
    %vpi_call 2 12 "$monitor", "%4t | %b %b | %b", $time, v0x606db023ec30_0, v0x606db023ed00_0, v0x606db023edd0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x606db023ec30_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x606db023ed00_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x606db023ec30_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x606db023ed00_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x606db023ec30_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x606db023ed00_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x606db023ec30_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x606db023ed00_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 18 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
