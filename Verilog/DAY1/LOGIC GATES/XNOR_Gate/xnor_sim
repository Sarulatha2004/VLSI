S_0x57cc85f92000 .scope module, "tb_xnor" "tb_xnor" 2 1;
 .timescale 0 0;
v0x57cc85fa3550_0 .var "a", 0 0;
v0x57cc85fa3620_0 .var "b", 0 0;
v0x57cc85fa36f0_0 .net "o", 0 0, L_0x57cc85fa3930;  1 drivers
S_0x57cc85f92190 .scope module, "uut" "xnor_gate" 2 6, 3 1 0, S_0x57cc85f92000;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "o";
L_0x57cc85fa37f0 .functor XOR 1, v0x57cc85fa3550_0, v0x57cc85fa3620_0, C4<0>, C4<0>;
L_0x57cc85fa3930 .functor NOT 1, L_0x57cc85fa37f0, C4<0>, C4<0>, C4<0>;
v0x57cc85f57490_0 .net *"_ivl_0", 0 0, L_0x57cc85fa37f0;  1 drivers
v0x57cc85fa32b0_0 .net "a", 0 0, v0x57cc85fa3550_0;  1 drivers
v0x57cc85fa3370_0 .net "b", 0 0, v0x57cc85fa3620_0;  1 drivers
v0x57cc85fa3410_0 .net "o", 0 0, L_0x57cc85fa3930;  alias, 1 drivers
    .scope S_0x57cc85f92000;
T_0 ;
    %vpi_call 2 9 "$dumpfile", "xnor_wave.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x57cc85f92000 {0 0 0};
    %vpi_call 2 12 "$display", "time | a b | o" {0 0 0};
    %vpi_call 2 13 "$monitor", "%4t | %b %b | %b", $time, v0x57cc85fa3550_0, v0x57cc85fa3620_0, v0x57cc85fa36f0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57cc85fa3550_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57cc85fa3620_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57cc85fa3550_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57cc85fa3620_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57cc85fa3550_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57cc85fa3620_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57cc85fa3550_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57cc85fa3620_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_0;
