:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x644c564a5fd0 .scope module, "tb_or" "tb_or" 2 1;
 .timescale 0 0;
v0x644c564b6fb0_0 .var "a", 0 0;
v0x644c564b7080_0 .var "b", 0 0;
v0x644c564b7150_0 .net "o", 0 0, L_0x644c564b7250;  1 drivers
S_0x644c564a6160 .scope module, "uut" "or_gate" 2 6, 3 1 0, S_0x644c564a5fd0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "o";
L_0x644c564b7250 .functor OR 1, v0x644c564b6fb0_0, v0x644c564b7080_0, C4<0>, C4<0>;
v0x644c564a6390_0 .net "a", 0 0, v0x644c564b6fb0_0;  1 drivers
v0x644c564b6dd0_0 .net "b", 0 0, v0x644c564b7080_0;  1 drivers
v0x644c564b6e90_0 .net "o", 0 0, L_0x644c564b7250;  alias, 1 drivers
    .scope S_0x644c564a5fd0;
T_0 ;
    %vpi_call 2 9 "$dumpfile", "or_wave.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x644c564a5fd0 {0 0 0};
    %vpi_call 2 12 "$display", "time | a b | o" {0 0 0};
    %vpi_call 2 13 "$monitor", "%4t | %b %b | %b", $time, v0x644c564b6fb0_0, v0x644c564b7080_0, v0x644c564b7150_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x644c564b6fb0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x644c564b7080_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x644c564b6fb0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x644c564b7080_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x644c564b6fb0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x644c564b7080_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x644c564b6fb0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x644c564b7080_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
