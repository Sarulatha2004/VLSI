#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5dcf115b8b60 .scope module, "tb_and" "tb_and" 2 1;
 .timescale 0 0;
v0x5dcf115dee70_0 .var "a", 0 0;
v0x5dcf115def40_0 .var "b", 0 0;
v0x5dcf115df010_0 .net "o", 0 0, L_0x5dcf115df110;  1 drivers
S_0x5dcf115cdfd0 .scope module, "uut" "and_gate" 2 7, 3 1 0, S_0x5dcf115b8b60;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "o";
L_0x5dcf115df110 .functor AND 1, v0x5dcf115dee70_0, v0x5dcf115def40_0, C4<1>, C4<1>;
v0x5dcf115ce200_0 .net "a", 0 0, v0x5dcf115dee70_0;  1 drivers
v0x5dcf115dec90_0 .net "b", 0 0, v0x5dcf115def40_0;  1 drivers
v0x5dcf115ded50_0 .net "o", 0 0, L_0x5dcf115df110;  alias, 1 drivers
    .scope S_0x5dcf115b8b60;
T_0 ;
    %vpi_call 2 10 "$dumpfile", "and_wave.vcd" {0 0 0};
    %vpi_call 2 11 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5dcf115b8b60 {0 0 0};
    %vpi_call 2 13 "$display", "time | a b | o" {0 0 0};
    %vpi_call 2 14 "$monitor", "%4t | %b %b | %b", $time, v0x5dcf115dee70_0, v0x5dcf115def40_0, v0x5dcf115df010_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5dcf115dee70_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5dcf115def40_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5dcf115dee70_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5dcf115def40_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5dcf115dee70_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5dcf115def40_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5dcf115dee70_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5dcf115def40_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
