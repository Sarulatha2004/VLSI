    %vpi_call 2 8 "$display", "Time :  a  b : o" {0 0 0};
    %vpi_call 2 9 "$monitor", " %0t :  %b %b : %b", $time, v0x5eb8936dc540_0, v0x5eb893726ac0_0, v0x5eb893726b80_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5eb8936dc540_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5eb893726ac0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5eb8936dc540_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5eb893726ac0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5eb8936dc540_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5eb893726ac0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5eb8936dc540_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5eb893726ac0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 14 "$finish" {0 0 0};
    %end;
