 .timescale 0 0;
v0x628e7cdef6d0_0 .var/real "delta", 0 0;
    .scope S_0x628e7cdef540;
T_0 ;
    %vpi_call 2 4 "$monitor", "time=%0t : delta=%.2f : delta=%0d", $time, v0x628e7cdef6d0_0, v0x628e7cdef6d0_0 {0 0 0};
    %pushi/real 1250000000, 4101; load=4.00000e+10
    %store/real v0x628e7cdef6d0_0;
    %delay 10, 0;
    %pushi/real 1170378588, 4067; load=2.18000
    %pushi/real 671089, 4045; load=2.18000
    %add/wr;
    %store/real v0x628e7cdef6d0_0;
    %delay 10, 0;
    %vpi_call 2 7 "$finish" {0 0 0};
    %end;
    .thread T_0;
