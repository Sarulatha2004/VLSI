S_0x6254c3b16560 .scope module, "bit_sel" "bit_sel" 2 1;
 .timescale 0 0;
v0x6254c3b166f0_0 .var "data", 7 0;
    .scope S_0x6254c3b16560;
T_0 ;
    %pushi/vec4 146, 0, 8;
    %store/vec4 v0x6254c3b166f0_0, 0, 8;
    %vpi_call 2 5 "$display", "time=%0t : data=%b : data[3]=%b", $time, v0x6254c3b166f0_0, &PV<v0x6254c3b166f0_0, 3, 1> {0 0 0};
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x6254c3b166f0_0, 4, 1;
    %vpi_call 2 7 "$display", "time=%0t  :  data=%b", $time, v0x6254c3b166f0_0 {0 0 0};
    %delay 10, 0;
    %vpi_call 2 8 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
