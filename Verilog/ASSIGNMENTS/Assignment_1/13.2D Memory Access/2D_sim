S_0x5fb55ebe6510 .scope module, "memory_2D" "memory_2D" 2 1;
 .timescale 0 0;
v0x5fb55ec0b190_0 .var/i "i", 31 0;
v0x5fb55ec331f0_0 .var/i "j", 31 0;
v0x5fb55ec332d0 .array "mem", 15 0, 7 0;
    .scope S_0x5fb55ebe6510;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5fb55ec0b190_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5fb55ec0b190_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_0.1, 5;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5fb55ec331f0_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x5fb55ec331f0_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_0.3, 5;
    %load/vec4 v0x5fb55ec0b190_0;
    %muli 4, 0, 32;
    %load/vec4 v0x5fb55ec331f0_0;
    %add;
    %pad/s 8;
    %load/vec4 v0x5fb55ec0b190_0;
    %pad/s 35;
    %pad/s 37;
    %muli 4, 0, 37;
    %pad/s 38;
    %load/vec4 v0x5fb55ec331f0_0;
    %pad/s 38;
    %add;
    %ix/vec4/s 4;
    %store/vec4a v0x5fb55ec332d0, 4, 0;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x5fb55ec331f0_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x5fb55ec331f0_0, 0, 32;
    %jmp T_0.2;
T_0.3 ;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x5fb55ec0b190_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x5fb55ec0b190_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %vpi_call 2 12 "$display", "-------2D memory-------" {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5fb55ec0b190_0, 0, 32;
T_0.4 ;
    %load/vec4 v0x5fb55ec0b190_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_0.5, 5;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5fb55ec331f0_0, 0, 32;
T_0.6 ;
    %load/vec4 v0x5fb55ec331f0_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_0.7, 5;
    %load/vec4 v0x5fb55ec0b190_0;
    %pad/s 35;
    %pad/s 37;
    %muli 4, 0, 37;
    %pad/s 38;
    %load/vec4 v0x5fb55ec331f0_0;
    %pad/s 38;
    %add;
    %ix/vec4/s 4;
    %load/vec4a v0x5fb55ec332d0, 4;
    %vpi_call 2 17 "$write", "%d\011", S<0,vec4,u8> {1 0 0};
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x5fb55ec331f0_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x5fb55ec331f0_0, 0, 32;
    %jmp T_0.6;
T_0.7 ;
    %vpi_call 2 19 "$write", "\012" {0 0 0};
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x5fb55ec0b190_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x5fb55ec0b190_0, 0, 32;
    %jmp T_0.4;
T_0.5 ;
    %vpi_call 2 21 "$display", "------ACCESS SPECIFIC ELEMENT-------" {0 0 0};
    %vpi_call 2 22 "$display", "mem[3][2]=%d(dec),%b(binary),%h(hex)", &A<v0x5fb55ec332d0, 14>, &A<v0x5fb55ec332d0, 14>, &A<v0x5fb55ec332d0, 14> {0 0 0};
    %end;
    .thread T_0;
